{"components/product/details":"\n<div class=\"product-details\" data-product-details data-product-title=\"{{product.title}}\">\n  {{#if show-breadcrumbs}}\n    {{> components/breadcrumbs breadcrumbs}}\n  {{/if}}\n\n  <div class=\"layout-container\">\n    <div class=\"product-column-left\">\n      {{> components/product/product-columns/images default_image=theme_settings.default_image}}\n    </div>\n    <div class=\"product-column-right\">\n      {{> components/product/product-columns/purchase schema_org=schema_org}}\n    </div>\n  </div>\n</div>\n\n{{#unless hide-tabs}}\n  {{> components/product/product-columns/tabs}}\n{{/unless}}\n","components/breadcrumbs":"\n{{#if this.length '>' 2}}\n  <div class=\"layout-container\">\n    <ol class=\"page-breadcrumbs\">\n      {{#each this}}\n        <li class=\"breadcrumb-item\">\n          {{#unless @first}}\n            {{# if name}}\n              <span class=\"breadcrumb-separator\">/</span>\n            {{/if}}\n          {{/unless}}\n\n          {{#all url name}}\n            <a class=\"breadcrumb-label breadcrumb-link\" href=\"{{url}}\">\n              <span>{{name}}</span>\n            </a>\n          {{else}}\n            {{# if name}}\n              <span class=\"breadcrumb-label {{#if @last}}is-active{{/if}}\">{{{limit name 1500}}}{{#if name.length '>' 150}}&hellip;{{/if}}</span>\n            {{/if}}\n          {{/all}}\n        </li>\n      {{/each}}\n    </ol>\n  </div>\n{{/if}}\n","components/product/product-columns/images":"<div class=\"product-slideshow {{#if product.images.length '>' 1}}visible-controls{{/if}}\">\n  <div class=\"product-slideshow-images\" data-slideshow-position=\"0\" data-product-images>\n    {{#each product.images}}\n      <div class=\"product-slideshow-image\" data-image-position=\"{{@index}}\" data-product-images-main>\n        <img src=\"{{getImage this '1024'}}\" alt=\"{{this.alt}}\">\n\n        {{#if ../theme_settings.product-images-enable-zoom}}\n          <a href=\"{{getImage this '1024'}}\" class=\"product-image-zoom\">\n            <span class=\"show-for-sr\">{{lang 'product.enlarge'}}</span>\n            <svg role=\"img\"><use xlink:href=\"#icon-enlarge\"></use></svg>\n          </a>\n        {{/if}}\n      </div>\n    {{/each}}\n\n    {{#if product.price.saved}}\n      <span class=\"product-single-sale-badge\">{{lang 'product.product_item_actions.sale'}}</span>\n    {{/if}}\n\n    <span class=\"loading-spinner\"></span>\n  </div>\n\n  {{#if product.images.length \">\" 1}}\n    <div class=\"product-thumbnails\">\n      {{#each product.images}}\n        <span\n          class=\"product-thumbnail {{#if @first}}active{{/if}}\"\n          style=\"background-image: url({{getImage this '200'}});\"\n          data-image-position=\"{{@index}}\"\n          data-high-res=\"{{getImage this '1024'}}\"\n          data-product-thumbnail>\n          <img class=\"show-for-sr\" src=\"{{getImage this '200'}}\" alt=\"{{this.alt}}\">\n        </span>\n      {{/each}}\n    </div>\n  {{/if}}\n</div>\n","components/product/product-columns/purchase":"<form\n  class=\"form\"\n  method=\"post\"\n  action=\"{{product.cart_url}}\"\n  enctype=\"multipart/form-data\"\n  data-cart-item-add\n  novalidate\n  data-product-options-count=\"{{product.options.length}}\">\n  <input type=\"hidden\" name=\"action\" value=\"add\">\n  <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\" data-product-id>\n\n  <div class=\"product-column-row\">\n    {{#if product.brand}}\n      <a class=\"product-brand\" href=\"{{product.brand.url}}\">{{product.brand.name}}</a>\n    {{/if}}\n    <h1 class=\"product-title\">{{product.title}}</h1>\n\n    <div class=\"product-price\">\n      {{#if product.call_for_price}}\n        {{product.call_for_price}}\n      {{else}}\n        {{#if product.price}}\n          {{> components/product/price\n            price=product.price\n            show_savings=true\n            show_tax_labels=true\n            schema_org=schema_org}}\n        {{/if}}\n      {{/if}}\n    </div>\n\n    <div class=\"product-description {{#unless quickshop}}has-excerpt{{/unless}}\" data-excerpt-length=\"220\">\n      {{{product.description}}}\n    </div>\n\n    {{#unless quickshop}}\n      {{#if product.description.length '>' 220}}\n        <a class=\"product-description-read-more\" href=\"#product-tab-description\" data-description-link>{{lang 'product.read_more'}}</a>\n      {{/if}}\n    {{/unless}}\n  </div>\n\n  {{#if product.options}}\n    <div data-product-option-change>\n      {{#each product.options}}\n        {{{dynamicComponent 'core/forms/options'}}}\n      {{/each}}\n    </div>\n  {{/if}}\n\n  {{#if product.customizations}}\n    <div data-product-option-change>\n      {{#each product.customizations}}\n        {{{dynamicComponent 'core/forms/customizations'}}}\n      {{/each}}\n    </div>\n  {{/if}}\n\n  {{> components/product/event}}\n\n  {{#if product.release_date}}\n    <div class=\"product-column-row\">\n      <span>{{product.release_date}}</span>\n    </div>\n  {{/if}}\n\n  <div class=\"product-column-row\">\n    {{#if product.show_quantity_input}}\n      <div class=\"product-quantity form-field\" data-product-quantity>\n        <label class=\"form-label\" for=\"quantity\">\n          <span class=\"form-label-text\">{{lang 'product.quantity'}}</span>\n          {{>components/common/quantity\n            min_purchase_quantity=product.min_purchase_quantity\n            max_purchase_quantity=product.max_purchase_quantity\n            value=1}}\n        </label>\n      </div>\n    {{else}}\n      <input type=\"hidden\" name=\"qty[]\" value=\"1\" />\n    {{/if}}\n\n    <div class=\"product-form-actions product-add-to-cart\" data-product-add>\n      {{#if product.can_purchase}}\n        <button class=\"product-add-button button button-large\" data-button-purchase {{#if product.pre_order}}data-button-preorder{{/if}} type=\"submit\">\n          <span data-button-text>\n            {{#if product.pre_order}}\n              {{lang 'product.pre_order'}}\n            {{else}}\n              {{lang 'product.add_to_cart'}}\n            {{/if}}\n          </span>\n          <span class=\"spinner\"></span>\n        </button>\n      {{else}}\n        <span class=\"button button-disabled button button-large\" data-button-text>\n          {{#if product.out_of_stock}}\n            {{#if product.out_of_stock_message}}\n              {{product.out_of_stock_message}}\n            {{else}}\n              {{lang 'product.out_of_stock_short'}}\n            {{/if}}\n          {{else}}\n            {{lang 'product.unavailable'}}\n          {{/if}}\n        </span>\n      {{/if}}\n\n      {{#if quickshop}}\n        <a class=\"button button-large secondary\" href=\"{{product.url}}\">\n          {{lang 'product.product_details'}}\n        </a>\n      {{/if}}\n    </div>\n\n  </div>\n\n  {{#unless quickshop}}\n    {{{snippet 'product_details'}}}\n  {{/unless}}\n</form>\n\n{{#if settings.show_wishlist}}\n  <form class=\"form-actions product-wishlist-form\" method=\"post\" action=\"{{product.add_to_wishlist_url}}\">\n    <input type=\"hidden\" name=\"variation_id\" value=\"\">\n    <button class=\"product-wishlist-button\" type=\"submit\">\n      <svg role=\"img\"><use xlink:href=\"#icon-increment-regular\"></use></svg>\n      {{lang 'product.add_to_wishlist'}}\n    </button>\n  </form>\n{{/if}}\n","components/product/product-columns/tabs":"<section class=\"module-product-tabs\">\n  <header class=\"module-product-tabs-header\">\n    <div class=\"layout-container\">\n      <ul class=\"product-tabs tabs\" data-tabs>\n        {{#if product.description}}\n          <li class=\"tab-title product-tab-title\" data-product-tab-title>\n            <a class=\"tab-link\" href=\"#product-tab-description\">{{lang 'product.tabs.description'}}</a>\n          </li>\n        {{/if}}\n        {{#if product.videos.list.length}}\n          <li class=\"tab-title product-tab-title\" data-product-tab-title>\n            <a class=\"tab-link\" href=\"#product-tab-videos\">{{lang 'product.tabs.videos'}}</a>\n          </li>\n        {{/if}}\n        <li class=\"tab-title product-tab-title\" data-product-tab-title>\n          <a class=\"tab-link\" href=\"#product-tab-information\">{{lang 'product.tabs.information'}}</a>\n        </li>\n\n        {{#if settings.show_product_reviews}}\n          <li class=\"tab-title product-tab-title\" data-product-tab-title>\n            <a class=\"tab-link\" href=\"#product-tab-reviews\">{{lang 'product.tabs.reviews'}}</a>\n          </li>\n        {{/if}}\n\n        {{#unless quickshop}}\n          <li class=\"tab-title product-tab-title product-share\">\n            {{> components/common/share-list\n              settings.add_this\n              cta=(lang 'product.share_product')\n              position='bottom'\n              url=product.url\n              title=product.title\n              image=(getImage product.main_image '1024' (cdn theme_settings.default_image))\n            }}\n          </li>\n        {{/unless}}\n      </ul>\n    </div>\n  </header>\n\n  <div class=\"tab-content product-tabs-content\">\n    <div class=\"layout-container\">\n      {{> components/loading-tab}}\n\n      {{#if product.description}}\n        <div class=\"tab-content-panel product-tabs-panel rte\" id=\"product-tab-description\">\n          {{{product.description}}}\n          {{{snippet 'product_description'}}}\n        </div>\n      {{/if}}\n\n      {{#if product.videos.list.length}}\n        <div class=\"tab-content-panel product-tabs-panel\" id=\"product-tab-videos\">\n          <div class=\"product-videos-list\">\n            {{#each product.videos.list}}\n              <div class=\"product-video-container\">\n                <iframe src=\"https://www.youtube.com/embed/{{id}}?rel=0&showinfo=0&autohide=1\" frameborder=\"0\" allowfullscreen></iframe>\n              </div>\n            {{/each}}\n          </div>\n        </div>\n      {{/if}}\n\n      <div class=\"tab-content-panel product-tabs-panel\" id=\"product-tab-information\">\n        <div class=\"product-information-block\">\n\n          {{#if product.availability}}\n            <div class=\"product-detail-item product-detail-availability\">\n              <span class=\"product-information-name\">{{lang 'product.availability'}}</span>\n              <span class=\"product-information-value\">{{product.availability}}</span>\n            </div>\n          {{/if}}\n\n          <div class=\"product-detail-item product-detail-stocklevel {{#unless product.stock_level}}product-details-hidden{{/unless}}\" data-product-stock>\n            <span class=\"product-information-name\">{{lang 'product.current_stock'}}</span>\n            <span class=\"product-information-value\"data-product-stock-level>{{product.stock_level}}</span>\n          </div>\n\n          {{#if product.shipping}}\n            <div class=\"product-detail-item product-detail-shipping\">\n              {{#if product.shipping.calculated}}\n                  <span class=\"product-information-name\">{{lang 'product.shipping'}}</span>\n                  <span class=\"product-information-value\">{{lang 'product.shipping_calculated'}}</span>\n              {{else}}\n                {{#if product.shipping.price.value '===' 0}}\n                  <span class=\"product-information-name\">{{lang 'product.shipping'}}</span>\n                  <span class=\"product-information-value\">{{lang 'product.shipping_free'}}</span>\n                {{else}}\n                  <span class=\"product-information-name\">{{lang 'product.shipping'}}</span>\n                  <span class=\"product-information-value\">{{lang 'product.shipping_fixed' amount=product.shipping.price.formatted}}</span>\n                {{/if}}\n              {{/if}}\n            </div>\n          {{/if}}\n\n          {{#if product.condition}}\n            <div class=\"product-detail-item product-detail-condition\">\n              <span class=\"product-information-name\">{{lang 'product.condition'}}</span>\n              <span class=\"product-information-value\">{{product.condition}}</span>\n            </div>\n          {{/if}}\n\n          {{#if product.custom_fields}}\n            <div class=\"product-detail-item product-detail-customfield\">\n              {{#each product.custom_fields}}\n                <span class=\"product-information-name\">{{name}}</span>\n                <span class=\"product-information-value\">{{value}}</span>\n              {{/each}}\n            </div>\n          {{/if}}\n\n          {{#if product.weight}}\n            <div class=\"product-detail-item\">\n              <span class=\"product-information-name\">{{lang 'product.weight'}}</span>\n              <span class=\"product-information-value\" data-product-weight>{{product.weight}}</span>\n            </div>\n          {{/if}}\n\n          {{#if product.width}}\n            <div class=\"product-detail-item product-detail-width\">\n              <span class=\"product-information-name\">{{lang 'product.width'}}</span>\n              <span class=\"product-information-value\">{{product.width}}</span>\n            </div>\n          {{/if}}\n\n          {{#if product.height}}\n            <div class=\"product-detail-item product-detail-height\">\n              <span class=\"product-information-name\">{{lang 'product.height'}}</span>\n              <span class=\"product-information-value\">{{product.height}}</span>\n            </div>\n          {{/if}}\n\n          {{#if product.depth}}\n            <div class=\"product-detail-item product-detail-depth\">\n              <span class=\"product-information-name\">{{lang 'product.depth'}}</span>\n              <span class=\"product-information-value\">{{product.depth}}</span>\n            </div>\n          {{/if}}\n\n          {{#if product.release_date}}\n            <div class=\"product-detail-item product-detail-releasedate\">\n              <span class=\"product-information-name\">{{lang 'product.release_date'}}</span>\n              <span class=\"product-information-value\">{{product.release_date}}</span>\n            </div>\n          {{/if}}\n\n          {{#if product.gtin}}\n            <div class=\"product-detail-item product-detail-gtin\">\n              <span class=\"product-information-name\">{{lang 'product.gtin'}}</span>\n              <span class=\"product-information-value\">{{product.gtin}}</span>\n            </div>\n          {{/if}}\n\n          {{#if product.mpn}}\n            <div class=\"product-detail-item product-detail-mpn\">\n              <span class=\"product-information-name\">{{lang 'product.mpn'}}</span>\n              <span class=\"product-information-value\">{{product.mpn}}</span>\n            </div>\n          {{/if}}\n\n          {{#if product.sku}}\n            <div class=\"product-detail-item product-detail-sku\">\n              <span class=\"product-information-name\">{{lang 'product.sku'}}</span>\n              <span class=\"product-information-value\" data-product-sku>{{product.sku}}</span>\n            </div>\n          {{/if}}\n\n          {{#if product.warranty}}\n            <div class=\"product-detail-item product-detail-warranty\">\n              <span class=\"product-information-name\">{{lang 'product.warranty'}}</span>\n              <span class=\"product-information-value\">{{nl2br product.warranty}}</span>\n            </div>\n          {{/if}}\n\n          {{#if product.detail_messages}}\n            <div class=\"product-detail-item product-detail-messages\">\n              <span class=\"product-information-name\">{{lang 'product.detail_messages'}}</span>\n              <span class=\"product-information-value\">{{{product.detail_messages}}}</span>\n            </div>\n          {{/if}}\n\n          {{#if product.bulk_discount_rates.length}}\n            <div class=\"product-detail-item product-detail-discount\">\n              <span class=\"product-information-name\">{{lang 'product.bulk_pricing.title'}}</span>\n              {{#each product.bulk_discount_rates}}\n                <span class=\"product-information-value\">\n                  {{lang 'product.bulk_pricing.range' min=min max=max}}\n                  {{#if type \"===\" \"percent\"}}\n                    {{lang 'product.bulk_pricing.percent' discount=discount.formatted}}\n                  {{/if}}\n                  {{#if type \"===\" \"fixed\"}}\n                    {{lang 'product.bulk_pricing.fixed' discount=discount.formatted}}\n                  {{/if}}\n                  {{#if type \"===\" \"price\"}}\n                    {{lang 'product.bulk_pricing.price' discount=discount.formatted}}\n                  {{/if}}\n                </span>\n              {{/each}}\n            </div>\n          {{/if}}\n\n          {{#if product.gift_wrapping_available}}\n            <div class=\"product-detail-item product-detail-giftwrap\">\n              <span class=\"product-information-name\">{{lang 'product.gift_wrapping'}}</span>\n              <span class=\"product-information-value\">{{lang 'product.gift_wrapping_available'}}</span>\n            </div>\n          {{/if}}\n\n        </div>\n      </div>\n\n      {{> components/product/product-columns/reviews}}\n    </div>\n  </div>\n</section>\n","components/common/share-list":"\n<div class=\"share-buttons show-list-{{position}}\" data-share-button-outer>\n  <a class=\"share-buttons-list-trigger\" href=\"#\" data-share-buttons>\n    <svg class=\"share-button-icon\" role=\"presentation\"><use xlink:href=\"#icon-share\"></use></svg>\n    <span class=\"share-button-text\">{{cta}}</span>\n  </a>\n\n  <ul class=\"share-buttons-list hidden\" data-share-list>\n    {{#each buttons}}\n\n      {{#if service '===' 'facebook'}}\n        <li>\n          <a class=\"share-button-item share-facebook\" target=\"_blank\" href=\"//www.facebook.com/sharer.php?u={{../../url}}\">\n            <svg role=\"img\"><use xlink:href=\"#icon-facebook\"></use></svg>\n            <span class=\"share-button-text\">{{lang 'share_list.facebook'}}</span>\n          </a>\n        </li>\n      {{/if}}\n\n      {{#if service '===' 'twitter'}}\n        <li>\n          <a class=\"share-button-item share-twitter\" target=\"_blank\" href=\"//twitter.com/share?url={{../../url}}\">\n            <svg role=\"img\"><use xlink:href=\"#icon-twitter\"></use></svg>\n            <span class=\"share-button-text\">{{lang 'share_list.twitter'}}</span>\n          </a>\n        </li>\n      {{/if}}\n\n      {{#if service '===' 'linkedin'}}\n        <li>\n          <a class=\"share-button-item share-linkedin\" target=\"_blank\" href=\"//www.linkedin.com/cws/share?url={{../../url}}\">\n            <svg role=\"img\"><use xlink:href=\"#icon-linkedin\"></use></svg>\n            <span class=\"share-button-text\">{{lang 'share_list.linkedin'}}</span>\n          </a>\n        </li>\n      {{/if}}\n\n      {{#if service '===' 'email'}}\n        <li>\n          <a class=\"share-button-item share-email\" href=\"mailto:?subject={{../../title}}&amp;body={{../../url}}\">\n            <svg role=\"img\"><use xlink:href=\"#icon-email\"></use></svg>\n            <span class=\"share-button-text\">{{lang 'share_list.email'}}</span>\n          </a>\n        </li>\n      {{/if}}\n\n      {{#if service '===' 'google_plusone'}}\n        <li>\n          <a class=\"share-button-item share-google\" target=\"_blank\" href=\"//plus.google.com/share?url={{../../url}}\">\n            <svg role=\"img\"><use xlink:href=\"#icon-google\"></use></svg>\n            <span class=\"share-button-text\">{{lang 'share_list.google_plus'}}</span>\n          </a>\n        </li>\n      {{/if}}\n\n      {{#if service '===' 'print'}}\n        <li>\n          <a class=\"share-button-item share-print\" href=\"#\" data-share-print>\n            <svg role=\"img\"><use xlink:href=\"#icon-printer\"></use></svg>\n            <span class=\"share-button-text\">{{lang 'share_list.print'}}</span>\n          </a>\n        </li>\n      {{/if}}\n\n      {{#if service '===' 'pinterest'}}\n        <li>\n          <a class=\"share-button-item share-pinterest\" target=\"_blank\" href=\"//pinterest.com/pin/create/button/?url={{../../url}}&amp;media={{../../image}}&amp;description={{../../title}}\">\n            <svg role=\"img\"><use xlink:href=\"#icon-pinterest\"></use></svg>\n            <span class=\"share-button-text\">{{lang 'share_list.pinterest'}}</span>\n          </a>\n        </li>\n      {{/if}}\n    {{/each}}\n\n    {{#each buttons}}\n      {{#if service '===' 'facebook_like'}}\n        <li>\n          <span class=\"share-button-item facebook-share-button\" data-facebook-like>\n            <div class=\"fb-like\"\n              data-href=\"{{../../url}}\"\n              data-layout=\"button_count\"\n              data-action=\"like\"\n              data-show-faces=\"false\">\n            </div>\n          </span>\n        </li>\n      {{/if}}\n    {{/each}}\n  </ul>\n</div>\n","components/loading-tab":"\n<div class=\"tab-content-panel active\" data-loading-tab>\n  <div class=\"tab-loading\">\n    <span class=\"loading-spinner\"></span>\n  </div>\n</div>\n","components/product/product-columns/reviews":"\n{{#if settings.show_product_reviews}}\n  <section class=\"tab-content-panel product-tabs-panel product-reviews\" id=\"product-tab-reviews\">\n    <header class=\"product-column-header\">\n      <h1 class=\"product-column-heading review-header\">\n        <span class=\"review-header-text\">\n          {{lang 'product.reviews.heading'}}\n        </span>\n\n        <span class=\"review-header-information\">\n          {{#if settings.show_product_rating}}\n            {{> components/common/rating-stars rating=product.rating star_size=\"medium\"}}\n            {{{snippet 'product_rating'}}}\n          {{/if}}\n\n          {{#if settings.show_product_reviews}}\n            {{#if settings.show_product_rating}}\n              <span class=\"review-count\">\n                {{{lang 'product.reviews.total_based_on' score=product.rating numReviews=product.num_reviews}}}\n              </span>\n            {{/if}}\n\n            <a href=\"#modal-review-form\" class=\"review-link\">\n              {{lang 'product.reviews.write_review'}}\n            </a>\n          {{/if}}\n        </span>\n      </h1>\n    </header>\n\n    {{{snippet 'reviews'}}}\n\n    {{#if product.num_reviews '>' 0}}\n      {{#each product.reviews.list}}\n        {{> components/reviews/review-item show_product_rating=../settings.show_product_rating}}\n      {{/each}}\n      {{#if product.num_reviews '>' 10 }}\n        <div class=\"product-column-row\" id=\"product-reviews-load-more\">\n          <a href=\"{{{../../pagination.reviews.next}}}\" class=\"button secondary button-large\" data-load-more-reviews>{{lang 'product.reviews.load_review'}}</a>\n        </div>\n      {{/if}}\n    {{else}}\n      <p>{{lang 'product.reviews.no_reviews_message'}}</p>\n    {{/if}}\n\n  </section>\n{{/if}}\n","components/product/price":"{{!\n  Shows a product's price.\n\n  Prices are fairly complicated. Besides having to be able to show the price\n  with and without tax, a merchant can also assign a Recommended Retail Price,\n  and a Sale Price.\n\n  This means that in extreme cases, we have to show 4 different prices at once.\n\n  When a Sale Price is set we do not have direct access to it--instead it\n  overwrites the regular price.\n\n  @param price object\n\n  @param show_tax_labels bool\n         If true, show a label saying the price is with or without tax.\n\n  @param show_savings bool\n         If true, show a label saying how much is saved from the full price.\n}}\n\n{{#if price}}\n  <div class=\"price\">\n\n    {{#if price.without_tax}}\n      <span class=\"price-withouttax\" data-product-price-wrapper=\"without-tax\">\n        {{#if price.rrp_without_tax}}\n          <span class=\"price-rrp\" data-price-rrp>\n            {{price.rrp_without_tax.formatted}}\n          </span>\n        {{/if}}\n\n        <span class=\"price-value\" data-product-price>\n          {{price.without_tax.formatted}}\n        </span>\n\n        {{#if show_tax_labels}}\n          {{#if price.with_tax}}\n            <span class=\"price-tax-label\">{{lang 'product.excluding_tax'}}</span>\n          {{/if}}\n        {{/if}}\n      </span>\n    {{/if}}\n\n\n    {{#if price.with_tax}}\n      <span class=\"price-withtax\" data-product-price-wrapper=\"with-tax\">\n        {{#if price.rrp_with_tax}}\n          <span class=\"price-rrp\" data-product-rrp>\n            {{price.rrp_with_tax.formatted}}\n          </span>\n        {{/if}}\n\n        <span class=\"price-value\" data-prodct-price>\n          {{price.with_tax.formatted}}\n        </span>\n\n        {{#if show_tax_labels}}\n          {{#if price.without_tax}}\n            <span class=\"price-tax-label\">{{lang 'product.including_tax'}}</span>\n          {{/if}}\n        {{/if}}\n      </span>\n    {{/if}}\n\n\n    {{#if show_savings}}\n      {{#if price.saved}}\n        <span class=\"price-saved\" data-product-price-saved>\n          {{lang 'core.price.you_save' amount=price.saved.formatted}}\n        </span>\n      {{/if}}\n    {{/if}}\n  </div>\n{{/if}}\n","components/common/quantity":"{{!\n  Quantity Widget with Increment/Decrement controls.\n\n  @param product\n         cart.item or product object\n}}\n\n<span class=\"form-field-quantity\" data-quantity-control=\"{{id}}\">\n  <input\n    class=\"form-input\"\n    type=\"number\"\n    name=\"qty{{#if product_id}}-{{id}}{{else}}[]{{/if}}\"\n    pattern=\"[0-9]*\"\n    value=\"{{#if quantity}}{{quantity}}{{else}}{{#if min_purchase_quantity}}{{min_purchase_quantity}}{{else}}1{{/if}}{{/if}}\"\n    min=\"{{#if min_purchase_quantity}}{{min_purchase_quantity}}{{else}}0{{/if}}\"\n    {{#if max_purchase_quantity '!==' 0}}max=\"{{max_purchase_quantity}}\"{{/if}}\n    data-quantity-control-input\n  >\n  <span class=\"quantity-control quantity-increment\" data-quantity-control-action=\"increment\">\n    <svg class=\"form-control-quantity\" role=\"presentation\">\n      <use xlink:href=\"#icon-increment-regular\"></use>\n    </svg>\n  </span>\n  <span class=\"quantity-control quantity-decrement\" data-quantity-control-action=\"decrement\">\n    <svg class=\"form-control-quantity\" role=\"presentation\">\n      <use xlink:href=\"#icon-decrement\"></use>\n    </svg>\n  </span>\n</span>\n","core/forms/options/date":"<div class=\"form-field form-field-options form-field-date{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"date\">\n  <span class=\"form-label\">\n\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper form-date-year\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][year]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.year'}}</option>\n          {{#if earliest_year '===' latest_year}}\n            <option value=\"{{earliest_year}}\" selected=\"selected\">{{earliest_year}}</option>\n          {{else}}\n            {{#for earliest_year latest_year}}\n              <option value=\"{{$index}}\" {{#if ../selected_date.year '==' $index}}selected=\"selected\"{{/if}}>{{$index}}</option>\n            {{/for}}\n          {{/if}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-month\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][month]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.month'}}</option>\n          {{#for 1 12}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.month '==' $index}}selected=\"selected\"{{/if}}>{{getShortMonth $index}}</option>\n          {{/for}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-day\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][day]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.day'}}</option>\n          {{#for 1 31}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.day '==' $index}}selected=\"selected\"{{/if}}>{{$index}}</option>\n          {{/for}}\n        </select>\n        </span>\n    </span>\n\n  </span>\n</div>\n","components/product/event":"{{#if product.event_date}}\n  <div class=\"product-column-row\">\n    <div class=\"form-field form-field-date form-required\">\n      <span class=\"form-label\">\n\n        <span class=\"form-field-title\">\n          {{product.event_date.name}}\n          <span class=\"required-text\">{{lang 'common.required'}}</span>\n        </span>\n\n        <span class=\"form-field-control\">\n          <span class=\"form-select-wrapper form-date-year\">\n            <span class=\"form-selected-text\">{{lang 'forms.date.year'}}</span>\n            <select class=\"form-input form-select\" name=\"EventDate[Yr]\" required>\n              <option disabled selected>{{lang 'forms.date.year'}}</option>\n              {{#if product.event_date.earliest_year '==' product.event_date.latest_year}}\n                <option value=\"{{product.event_date.earliest_year}}\">{{product.event_date.earliest_year}}</option>\n              {{else}}\n                {{#for product.event_date.earliest_year product.event_date.latest_year}}\n                  <option value=\"{{$index}}\">{{$index}}</option>\n                {{/for}}\n              {{/if}}\n            </select>\n          </span>\n\n          <span class=\"form-select-wrapper form-date-month\">\n            <span class=\"form-selected-text\">{{lang 'forms.date.month'}}</span>\n            <select class=\"form-input form-select\" name=\"EventDate[Mth]\" required>\n              <option disabled>{{lang 'forms.date.month'}}</option>\n              {{#for 1 12}}\n                <option value=\"{{$index}}\">{{getShortMonth $index}}</option>\n              {{/for}}\n            </select>\n          </span>\n\n          <span class=\"form-select-wrapper form-date-day\">\n            <span class=\"form-selected-text\">{{lang 'forms.date.day'}}</span>\n            <select class=\"form-input form-select\" name=\"EventDate[Day]\" required>\n              <option disabled>{{lang 'forms.date.day'}}</option>\n              {{#for 1 31}}\n                <option value=\"{{$index}}\">{{$index}}</option>\n              {{/for}}\n            </select>\n          </span>\n        </span>\n\n      </span>\n\n      {{#if product.event_date.date_start}}\n        {{#if product.event_date.date_end}}\n          <span class=\"product-event-bounds\">{{lang 'product.event_bounds' start_date=product.event_date.date_start end_date=product.event_date.date_end}}</span>\n        {{/if}}\n      {{/if}}\n\n    </div>\n  </div>\n{{/if}}\n","core/forms/options/input-numbers":"\n<div class=\"form-field form-field-options form-field-number{{#if required}} form-required{{/if}}\"\n data-product-attribute=\"input-number\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\" id=\"attribute-{{id}}\" name=\"attribute[{{id}}]\" type=\"number\" value=\"{{prefill}}\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","core/forms/options/product-list":"\n<div class=\"form-field form-field-options form-field-product-list{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"product-list\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    {{#unless values.0.image}}\n      {{!-- same as radio buttons if no image set --}}\n      {{#each values}}\n        <label class=\"form-label\" data-product-attribute-value=\"{{id}}\">\n          <input\n            class=\"form-input form-radio\"\n            name=\"attribute[{{../id}}]\"\n            type=\"radio\"\n            value=\"{{id}}\"\n            {{#if selected}}checked{{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\">\n          <span class=\"form-label-text\">{{label}}</span>\n        </label>\n      {{/each}}\n      {{#unless required}}\n        <label class=\"form-label\">\n          <input\n            class=\"form-input form-radio\"\n            name=\"attribute[{{id}}]\"\n            type=\"radio\"\n            value=\"0\"\n            {{#if prefill '==' 0}}checked{{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\">\n          <span class=\"form-label-text\">{{lang 'product.none'}}</span>\n        </label>\n      {{/unless}}\n    {{else}}\n      {{#each values}}\n        <label class=\"form-label product-picklist-item\" data-product-attribute-value=\"{{id}}\">\n          <input\n            class=\"form-input form-radio product-picklist-radio\"\n            name=\"attribute[{{../id}}]\"\n            type=\"radio\" value=\"{{id}}\"\n            {{#if selected}}checked{{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\">\n          <span class=\"product-picklist-item-body\">\n            <span class=\"product-picklist-image-wrap\">\n              <img class=\"product-picklist-image\" src=\"{{getImage image 'core-product-picklist' (cdn ../theme_settings.default_image)}}\" alt=\"{{image.alt}}\">\n            </span>\n            <span class=\"product-picklist-label-text form-label-text\">{{label}}</span>\n          </span>\n        </label>\n      {{/each}}\n      {{#unless required}}\n        <label class=\"form-label product-picklist-item\">\n          <input\n            class=\"form-input form-radio product-picklist-radio\"\n            name=\"attribute[{{id}}]\"\n            type=\"radio\"\n            value=\"0\"\n            {{#if prefill '==' 0}}checked{{/if}}>\n          <span class=\"product-picklist-item-body picklist-none\">\n            <span class=\"product-picklist-label-text form-label-text\">{{lang 'product.none'}}</span>\n          </span>\n        </label>\n      {{/unless}}\n    {{/unless}}\n  </div>\n</div>\n","core/forms/options/input-text":"\n<div class=\"form-field form-field-options form-field-text{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"input-text\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        type=\"text\"\n        placeholder=\"{{prefill}}\"\n        value=\"\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","core/forms/options/set-rectangle":"\n<div class=\"form-field form-field-options form-field-rectangle{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"set-rectangle\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label class=\"form-label rectangle\" data-product-attribute-value=\"{{id}}\">\n        <input\n          class=\"form-input form-rectangle\"\n          name=\"attribute[{{../id}}]\"\n          id=\"attribute-{{id}}\"\n          type=\"radio\"\n          value=\"{{id}}\"\n          {{#if selected}}checked{{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n        <span class=\"rectangle-text form-label-text\">{{label}}</span>\n      </label>\n    {{/each}}\n  </div>\n</div>\n","core/forms/options/set-radio":"\n<div class=\"form-field form-field-options form-field-radio{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"set-radio\">\n  <div class=\"form-field-title product-option-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label class=\"form-label\" data-product-attribute-value=\"{{id}}\">\n        <input\n          class=\"form-input form-radio\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{../id}}]\"\n          type=\"radio\"\n          value=\"{{id}}\"\n          {{#if selected}}checked{{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n        <span class=\"form-label-text\">{{label}}</span>\n      </label>\n    {{/each}}\n  </div>\n</div>\n","core/forms/options/swatch":"\n<div class=\"form-field form-field-options form-field-swatch{{#if required}} form-required{{/if}}\"\n  data-swatch-selector\n  data-product-attribute=\"swatch\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    <span class=\"swatch-value\" data-swatch-value>{{lang 'forms.swatches_none_selected'}}</span>\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label class=\"swatch-wrap\" data-swatch-value=\"{{label}}\" data-product-attribute-value=\"{{id}}\">\n        <span class=\"form-label-text\">{{label}}</span>\n\n        <input\n          class=\"form-input swatch-radio\"\n          id=\"attribute-{{id}}\"\n          type=\"radio\"\n          name=\"attribute[{{../id}}]\"\n          value=\"{{id}}\"\n          {{#if selected}}checked{{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n\n        <span class=\"swatch {{#if data.[1]}}two-colors{{/if}}{{#if data.[2]}} three-colors{{/if}}\">\n          {{#if pattern}}\n            <span class=\"swatch-color swatch-pattern\" style=\"background-image: url('{{getImage image 'core-swatch'}}');\"></span>\n          {{/if}}\n\n          {{#if data.[0]}}\n            <span class=\"swatch-color\" style=\"background-color: #{{data.[0]}}\"></span>\n          {{/if}}\n\n          {{#if data.[1]}}\n            <span class=\"swatch-color secondary\" style=\"background-color: #{{data.[1]}}\"></span>\n          {{/if}}\n\n          {{#if data.[2]}}\n            <span class=\"swatch-color tertiary\" style=\"background-color: #{{data.[2]}}\"></span>\n          {{/if}}\n        </span>\n\n        {{#if pattern}}\n          <span class=\"swatch-pattern-expanded\">\n            <img class=\"swatch-pattern-image\" src=\"{{getImage image 'core-swatch'}}\">\n          </span>\n        {{/if}}\n\n      </label>\n    {{/each}}\n  </div>\n\n</div>\n","core/forms/options/textarea":"\n<div class=\"form-field form-field-options form-field-textarea{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"textarea\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <textarea\n        class=\"form-input\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        rows=\"4\"\n        placeholder=\"{{prefill}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"></textarea>\n    </span>\n  </label>\n</div>\n","core/forms/options/set-select":"\n<div class=\"form-field form-field-options form-field-select{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"set-select\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <div class=\"form-select-wrapper\">\n        <select\n          class=\"form-input form-select\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{id}}]\"\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n          <option value=\"\" {{#any values selected=true}}{{else}}selected{{/any}} disabled>{{lang 'product.pick_one'}}</option>\n          {{#each values}}\n            <option value=\"{{id}}\" {{#if selected}}selected{{/if}} data-product-attribute-value=\"{{id}}\">{{label}}</option>\n          {{/each}}\n        </select>\n      </div>\n    </span>\n  </label>\n</div>\n","core/forms/customizations/checkbox":"\n<div class=\"form-field form-field-customizations form-field-checkbox{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-control\">\n      <input class=\"form-input form-checkbox\"\n        id=\"product-field-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        type=\"checkbox\"\n        {{#if value}}checked{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n      <span class=\"form-label-text product-customization-text\">\n        {{display_name}}\n        {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n      </span>\n    </span>\n  </label>\n</div>\n","core/forms/customizations/file":"\n<div class=\"form-field form-field-customizations form-field-file {{#if required}}form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-file-wrapper\" data-button-label=\"{{lang 'core.account.products.upload'}}\">\n        <input\n          class=\"form-input form-file\"\n          id=\"product-fields-{{id}}\"\n          name=\"ProductFields[{{this.id}}]\"\n          type=\"file\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n      </span>\n    </span>\n  </label>\n\n  {{#if file_types '!==' ''}}\n    <span class=\"form-file-types\">{{lang 'core.account.products.filetypes' file_types=file_types}}</span>\n  {{/if}}\n\n  <span class=\"form-file-size\">{{lang 'core.account.products.max_filesize' bytes=file_size}}</span>\n</div>\n","core/forms/options/input-file":"\n<div class=\"form-field form-field-options form-field-file {{#if required}}form-required{{/if}}\"\n  data-product-attribute=\"input-file\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-file-wrapper\" data-button-label=\"{{lang 'core.account.products.upload'}}\">\n        <input\n          class=\"form-input form-file\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{id}}]\"\n          type=\"file\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n      </span>\n    </span>\n  </label>\n\n  {{#if file_types '!==' ''}}\n    <span class=\"form-file-types\">{{lang 'core.account.products.filetypes' file_types=file_types}}</span>\n  {{/if}}\n\n  <span class=\"form-file-size\">{{lang 'core.account.products.max_filesize' bytes=file_size}}</span>\n</div>\n","core/forms/options/input-checkbox":"\n<div class=\"form-field form-field-options form-field-checkbox{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"input-checkbox\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    <label class=\"form-label {{class}}\">\n      <input\n        class=\"form-input form-checkbox\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        type=\"checkbox\"\n        value=\"{{value}}\"\n        {{#if checked}}checked{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n      <span class=\"form-label-text\">{{label}}</span>\n    </label>\n  </div>\n</div>\n","core/forms/customizations/select":"\n<div class=\"form-field form-field-customizations form-field-select{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper\">\n        <select\n          class=\"form-input input-select\"\n          id=\"product-fields-{{id}}\"\n          name=\"ProductFields[{{this.id}}]\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n          <option value=\"\" {{#any values selected=true}}{{else}}selected{{/any}} disabled>{{lang 'product.pick_one'}}</option>\n          {{#each values}}\n            <option value=\"{{this}}\" {{#if selected}}selected{{/if}}>{{this}}</option>\n          {{/each}}\n        </select>\n      </span>\n    </span>\n  </label>\n</div>\n","core/forms/customizations/text":"\n<div class=\"form-field form-field-customizations form-field-text{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\" for=\"product-fields-{{id}}\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"product-fields-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        type=\"text\"\n        placeholder=\"{{prefill}}\"\n        value=\"{{value}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","core/forms/customizations/textarea":"\n<div class=\"form-field form-field-customizations form-field-textarea{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}:\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <textarea\n        class=\"form-input\"\n        id=\"product-fields-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        rows=\"4\"\n        placeholder=\"{{prefill}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"></textarea>\n    </span>\n  </label>\n</div>\n","components/common/rating-stars":"<div class=\"rating-stars {{#if star_size}}{{star_size}}{{else}}small{{/if}}-stars\" aria-hidden=\"true\">\n  {{#for 1 5}}\n    <svg class=\"rating-star {{#if ../rating '>=' $index}}rating-star-full{{else}}rating-star-empty{{/if}}\" role=\"presentation\">\n      <use xlink:href=\"#icon-star\"></use>\n    </svg>\n  {{/for}}\n</div>\n","components/reviews/review-item":"<article class=\"product-column-row review-item\">\n  <header class=\"review-item-header\">\n    <h5 class=\"review-item-title\">{{title}}</h5>\n    {{#if show_product_rating}}\n      <div class=\"review-item-stars\">\n        {{> components/common/rating-stars rating=rating star_size=\"medium\"}}\n        <span class=\"review-item-score\">{{lang 'product.reviews.stars_count' NUM=rating}}</span>\n      </div>\n    {{/if}}\n  </header>\n  <p class=\"review-item-body\">{{text}}</p>\n</article>\n"}
