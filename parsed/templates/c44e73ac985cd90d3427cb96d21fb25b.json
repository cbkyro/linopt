{"components/product/product-rating":"<!--\n\n  Readonly star ratings\n\n  {{> components/product/product-rating rating=this.product.rating total=this.product.num_reviews}}\n\n-->\n\n\n<div class=\"product-rating\">\n  <div class=\"rating-stars-readonly\" aria-hidden=\"true\">\n    {{> components/common/rating-stars rating=rating}}\n  </div>\n  <span class=\"rating-total-score\">{{this.rating}}</span>\n  <span class=\"rating-total-votes\">(<a href=\"#\">{{this.total}}</a>)</span>\n</div>\n","components/common/rating-stars":"<div class=\"rating-stars {{#if star_size}}{{star_size}}{{else}}small{{/if}}-stars\" aria-hidden=\"true\">\n  {{#for 1 5}}\n    <svg class=\"rating-star {{#if ../rating '>=' $index}}rating-star-full{{else}}rating-star-empty{{/if}}\" role=\"presentation\">\n      <use xlink:href=\"#icon-star\"></use>\n    </svg>\n  {{/for}}\n</div>\n"}
