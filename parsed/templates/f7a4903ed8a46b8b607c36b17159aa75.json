{"components/footer/newsletter-action":"<section class=\"footer-newsletter\">\n  <h1 class=\"show-for-sr\">{{lang 'newsletter_form.heading'}}</h1>\n  <div class=\"layout-container\">\n    <form class=\"footer-newsletter-form\" action=\"{{urls.subscribe.action}}\" data-validated-form method=\"post\">\n      <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n      <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n      <input type=\"hidden\" name=\"check\" value=\"1\">\n\n      {{> core/forms/common/email id='nl_email' name='nl_email' label=(lang 'forms.newsletter.email') prefill=(lang 'forms.newsletter.placeholder') required=true value=customer.email}}\n      <div class=\"form-action\">\n        <input type=\"submit\" class=\"button button-wide\" value=\"{{lang 'newsletter_form.submit_form'}}\">\n      </div>\n    </form>\n  </div>\n</section>\n","core/forms/common/email":"\n<div class=\"form-field form-field-common form-field-email {{#if required}} form-required{{/if}}\" id=\"{{id}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{label}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"input-{{id}}\"\n        name=\"{{name}}\"\n        type=\"email\"\n        placeholder=\"{{prefill}}\"\n        {{#if value}}value=\"{{value}}\"{{/if}}\n        {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n"}
