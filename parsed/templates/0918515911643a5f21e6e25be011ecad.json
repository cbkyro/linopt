{"components/currency-selector":"\n<div class=\"form-field form-field-select currency-selector\" data-currency-selector>\n  <label class=\"form-label\">\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper\">\n        <span class=\"form-selected-text\">\n          {{{currency_selector.active_currency_code}}} \n        </span>\n        <select>\n          {{#each currency_selector.currencies}}\n            <option value=\"{{{switch_url}}}\" {{#if is_active}}selected{{/if}}>\n              {{name}}\n            </option>\n          {{/each}}\n        </select>\n      </span>\n    </span>\n  </label>\n</div>\n"}
