{"components/reviews/review-item":"<article class=\"product-column-row review-item\">\n  <header class=\"review-item-header\">\n    <h5 class=\"review-item-title\">{{title}}</h5>\n    {{#if show_product_rating}}\n      <div class=\"review-item-stars\">\n        {{> components/common/rating-stars rating=rating star_size=\"medium\"}}\n        <span class=\"review-item-score\">{{lang 'product.reviews.stars_count' NUM=rating}}</span>\n      </div>\n    {{/if}}\n  </header>\n  <p class=\"review-item-body\">{{text}}</p>\n</article>\n","components/common/rating-stars":"<div class=\"rating-stars {{#if star_size}}{{star_size}}{{else}}small{{/if}}-stars\" aria-hidden=\"true\">\n  {{#for 1 5}}\n    <svg class=\"rating-star {{#if ../rating '>=' $index}}rating-star-full{{else}}rating-star-empty{{/if}}\" role=\"presentation\">\n      <use xlink:href=\"#icon-star\"></use>\n    </svg>\n  {{/for}}\n</div>\n"}
