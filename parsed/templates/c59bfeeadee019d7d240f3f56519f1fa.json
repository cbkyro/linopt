{"components/cart/shipping-quotes":"<form class=\"form\">\n  <div class=\"modal-form-contents\">\n    {{#if quotes.shipping_methods}}\n      {{#each quotes.shipping_methods}}\n        {{> components/cart/shipping-quote}}\n      {{/each}}\n    {{/if}}\n\n    {{#if quotes.ups_shipping_methods}}\n      {{#each quotes.ups_shipping_methods}}\n        {{> components/cart/shipping-quote provider_name=(concat provider_name (concat \": \" description))}}\n      {{/each}}\n    {{/if}}\n  </div>\n\n  <div class=\"modal-footer\">\n    <input class=\"button\" type=\"submit\" value=\"{{lang 'cart.shipping_calculator.select'}}\">\n  </div>\n</form>\n","components/cart/shipping-quote":"\n<div class=\"form-field form-field-radio form-required\">\n  <div class=\"form-field-control\">\n    <label for=\"shipping-quote-{{id}}\" class=\"form-label\">\n      <input id=\"shipping-quote-{{id}}\" class=\"form-input form-radio\" type=\"radio\" name=\"shipping_method\" value=\"{{id}}\" data-shipping-quote>\n      <span class=\"form-label-text\">{{#if logo_path}}<img src=\"{{logo_path}}\" class=\"shipping-method-badge\">{{/if}} {{provider_name}} {{cost.formatted}}</span>\n    </label>\n  </div>\n</div>\n"}
