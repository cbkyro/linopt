{"components/cart/content/options-configurable":"<div class=\"cart-item-option\">\n  <span class=\"cart-item-option-name\">\n    {{name}}:\n  </span>\n\n  <span class=\"cart-item-option-value\">\n    {{#if value '===' 'checked'}}\n      {{lang 'cart.checked'}}\n    {{else}}\n      {{#if is_file}}\n        <a href=\"/viewfile.php?prodfield={{../id}}&cartitem={{../../id}}\">{{{value}}}</a>\n      {{else}}\n        {{value}}\n      {{/if}}\n    {{/if}}\n  </span>\n</div>\n"}
