{"components/carousel":"<section class=\"carousel\n  {{#if theme_settings.carousel-transition '===' 'slide-transition'}}slide-transition{{else}}fade-transition{{/if}}\n  {{#if carousel.slides.length '>' 1}}display-controls{{/if}}\">\n\n  <div class=\"carousel-slides\" data-carousel-slides>\n    {{#each carousel.slides}}\n      <figure class=\"carousel-item\">\n        <a class=\"carousel-item-image\" href=\"{{#if url}}{{url}}{{else}}#{{/if}}\">\n          <img src=\"{{image}}\" {{#if alt_text}}alt=\"{{{alt_text}}}\"{{/if}} />\n        </a>\n\n        {{#any heading text button_text}}\n          <figcaption class=\"carousel-item-info\">\n            {{#if heading}}\n              <h2 class=\"carousel-item-heading\">{{{heading}}}</h2>\n            {{/if}}\n\n            {{#if text}}\n              <p class=\"carousel-item-description\">{{{text}}}</p>\n            {{/if}}\n\n            {{#if button_text}}\n              <a class=\"carousel-item-button button\" href=\"{{#if url}}{{url}}{{else}}#{{/if}}\">{{{button_text}}}</a>\n            {{/if}}\n          </figcaption>\n        {{/any}}\n      </figure>\n    {{/each}}\n  </div>\n\n</section>\n"}
