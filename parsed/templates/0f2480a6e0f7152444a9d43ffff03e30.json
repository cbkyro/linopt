{"components/reviews/input-star":"\n<input class=\"form-input form-radio\" type=\"radio\" id=\"{{rating_number}}-stars\" name=\"revrating\" value=\"{{rating_number}}\" data-vd-message-required=\"{{lang 'forms.validation.required_rating'}}\" required>\n<label for=\"{{rating_number}}-stars\" class=\"star\" title=\"{{lang 'product.reviews.stars_count' NUM=rating_number}}\">\n  <svg class=\"star star-full\" role=\"presentation\">\n    <use xlink:href=\"#icon-star\"></use>\n  </svg>\n  <span class=\"span show-for-sr\">{{lang 'product.reviews.stars_count' NUM=rating_number}}</span>\n</label>\n"}
