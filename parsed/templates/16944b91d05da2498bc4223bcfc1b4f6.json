{"components/category/header":"\n{{#if category.subcategories}}\n  <div class=\"collection-navigation-list\">\n    {{> components/category/category-menu}}\n  </div>\n{{/if}}\n\n<div class=\"collection-sortby\">\n    {{#if theme_settings.collection-enable-filters}}\n      {{> components/faceted-search/facets-toggle}}\n    {{/if}}\n      \n    {{#if category.products}}\n      {{> components/common/collections/sort-by sort=pagination.category.sort}}\n    {{/if}}\n</div>\n","components/category/category-menu":"\n{{#if category.subcategories }}\n  <ul>\n    {{#each category.subcategories}}\n      <li>\n        <a href=\"{{url}}\">{{name}}</a>\n      </li>\n    {{/each}}\n  </ul>\n{{/if}}\n","components/faceted-search/facets-toggle":"\n<button class=\"collection-toggle-facets\" type=\"button\" data-faceted-search-toggle>\n  {{lang 'collections.filters'}}\n  {{> svg/icon-increment role='button'}}\n</button>\n","components/common/collections/sort-by":"<form class='collections-sort-by-form' method=\"get\" data-sort-by>\n  <span class=\"form-field form-inline\">\n    <label class=\"form-label select-wrapper\" for=\"sort\">\n      <span class=\"form-select-wrapper\">\n        <span class=\"form-selected-text\">\n          {{#unless sort}}\n            {{lang 'collections.sort_by.select_sorting'}}\n          {{else}}\n            {{lang 'collections.sort_by.heading'}}\n          {{/unless}}\n          {{#if sort \"===\" \"featured\"}}\n            {{lang 'collections.sort_by.featured'}}\n          {{/if}}\n          {{#if sort \"===\" \"newest\"}}\n            {{lang 'collections.sort_by.newest'}}\n          {{/if}}\n          {{#if sort \"===\" \"bestselling\"}}\n            {{lang 'collections.sort_by.popular'}}\n          {{/if}}\n          {{#if sort \"===\" \"alphaasc\"}}\n            {{lang 'collections.sort_by.alpha_asc'}}\n          {{/if}}\n          {{#if sort \"===\" \"alphadesc\"}}\n            {{lang 'collections.sort_by.alpha_desc'}}\n          {{/if}}\n          {{#if sort \"===\" \"avgcustomerreview\"}}\n            {{lang 'collections.sort_by.rating'}}\n          {{/if}}\n          {{#if sort \"===\" \"priceasc\"}}\n            {{lang 'collections.sort_by.price_asc'}}\n          {{/if}}\n          {{#if sort \"===\" \"pricedesc\"}}\n            {{lang 'collections.sort_by.price_desc'}}\n          {{/if}}\n          {{#if sort \"===\" \"relevance\"}}\n            {{lang 'collections.sort_by.relevance'}}\n          {{/if}}\n        </span>\n\n        <select class=\"form-input input-select\" name=\"sort\" id=\"sort\">\n          <option {{#unless sort}}selected{{/unless}} disabled>{{lang 'collections.sort_by.select_sorting'}}</option>\n          <option value=\"featured\" {{#if sort \"===\" \"featured\"}}selected{{/if}}>{{lang 'collections.sort_by.featured'}}</option>\n          <option value=\"newest\" {{#if sort \"===\" \"newest\"}}selected{{/if}}>{{lang 'collections.sort_by.newest'}}</option>\n          <option value=\"bestselling\" {{#if sort \"===\" \"bestselling\"}}selected{{/if}}>{{lang 'collections.sort_by.popular'}}</option>\n          <option value=\"alphaasc\" {{#if sort \"===\" \"alphaasc\"}}selected{{/if}}>{{lang 'collections.sort_by.alpha_asc'}}</option>\n          <option value=\"alphadesc\" {{#if sort \"===\" \"alphadesc\"}}selected{{/if}}>{{lang 'collections.sort_by.alpha_desc'}}</option>\n          <option value=\"avgcustomerreview\" {{#if sort \"===\" \"avgcustomerreview\"}}selected{{/if}}>{{lang 'collections.sort_by.rating'}}</option>\n          <option value=\"priceasc\" {{#if sort \"===\" \"priceasc\"}}selected{{/if}}>{{lang 'collections.sort_by.price_asc'}}</option>\n          <option value=\"pricedesc\" {{#if sort \"===\" \"pricedesc\"}}selected{{/if}}>{{lang 'collections.sort_by.price_desc'}}</option>\n          {{#if result_count}}\n            <option value=\"relevance\" {{#if sort \"===\" \"relevance\"}}selected{{/if}}>{{lang 'collections.sort_by.relevance'}}</option>\n          {{/if}}\n        </select>\n      </span>\n    </label>\n    </span>\n</form>\n","svg/icon-increment":"\n{{#unless hide-wrapper}}\n  <svg\n    {{#if role}}role=\"{{role}}\"{{/if}}\n    version=\"1.1\"\n    xmlns=\"http://www.w3.org/2000/svg\"\n    xmlns:xlink=\"http://www.w3.org/1999/xlink\"\n    viewBox=\"0 0 32 32\">\n{{/unless}}\n<path fill-rule=\"evenodd\" class=\"icon-increment-vertical\" d=\"M19.037 3.544h-6.076v24.911h6.076v-24.911z\"></path>\n<path fill-rule=\"evenodd\" class=\"icon-increment-horizontal\" d=\"M3.999 13.266v5.468h24v-5.468h-24z\"></path>\n{{#unless hide-wrapper}}\n  </svg>\n{{/unless}}\n"}
