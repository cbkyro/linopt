{"components/faceted-search/facets/multi":"<div class=\"collection-facet-list-wrapper\" data-facet-filter-wrapper>\n  <ul\n    data-facet-name=\"{{facet}}\"\n    data-count=\"{{items_count}}\"\n    data-has-more-results=\"{{has_more_results}}\">\n\n    {{#if items}}\n      {{#each items}}\n        <li class=\"collection-facet-list-item {{#if @index '>=' 5}}hidden{{/if}} {{#if selected}}is-active{{/if}}\" data-facet-item>\n          <a href=\"{{#if remove_url}}{{remove_url}}{{else}}{{url}}{{/if}}\" data-faceted-search-facet rel=\"nofollow\">\n            {{title}}\n\n            {{#if ../../show_product_counts}}\n              <span data-facet-item-count>\n                ({{count}})\n              </span>\n            {{/if}}\n          </a>\n        </li>\n      {{/each}}\n    {{else}}\n      <li>{{lang 'faceted_search.empty_facet'}}</li>\n    {{/if}}\n\n  </ul>\n\n   {{> components/faceted-search/facets-additional-toggle}}\n</div>\n","components/faceted-search/facets-additional-toggle":"\n{{#if show_more_toggle}}\n  <div class=\"collection-facet-list-item collection-facet-list-separation\" data-facet-more>\n    <button\n      class=\"collection-facet-list-toggle\"\n      role=\"button\"\n      data-additional-facets=\"{{facet}}\">\n      <span>{{lang 'faceted_search.show_more'}}</span>\n      <span class=\"hidden\">{{lang 'faceted_search.show_less'}}</span>\n    </button>\n  </div>\n{{/if}}"}
