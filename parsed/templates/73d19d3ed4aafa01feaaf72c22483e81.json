{"components/faceted-search/facets/rating":"<ul class=\"collection-facet-list\" data-facet-filter-wrapper>\n  {{#each items}}\n    {{#if param_value}}\n      <li class=\"collection-facet-list-item {{#if selected}}is-active{{/if}}\" data-facet-item>\n        <a href=\"{{#if remove_url}}{{remove_url}}{{else}}{{url}}{{/if}}\" data-faceted-search-facet rel=\"nofollow\">\n          {{> components/common/rating-stars rating=param_value star_size='medium'}}\n\n          {{#if ../../show_product_counts}}{{#if count}}<span data-facet-item-count>({{count}})</span>{{/if}}{{/if}}\n        </a>\n      </li>\n    {{/if}}\n  {{/each}}\n</ul>\n","components/common/rating-stars":"<div class=\"rating-stars {{#if star_size}}{{star_size}}{{else}}small{{/if}}-stars\" aria-hidden=\"true\">\n  {{#for 1 5}}\n    <svg class=\"rating-star {{#if ../rating '>=' $index}}rating-star-full{{else}}rating-star-empty{{/if}}\" role=\"presentation\">\n      <use xlink:href=\"#icon-star\"></use>\n    </svg>\n  {{/for}}\n</div>\n"}
